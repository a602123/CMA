@model CMA.Common.Model.DEMSParamter

<div class="form-body">
    <div class="form-group">
        <label class="col-md-2 control-label">名称</label>
        <div class="col-md-4">
            @Html.TextBoxFor(m => m.DEMSName, new { @placeholder = "名称", @required = "required", @class = "form-control" })
        </div>
    </div>

    <div class="form-group">
        <label class="col-md-2 control-label">主机IP</label>
        <div class="col-md-4">
            @Html.TextBoxFor(m => m.Host, new { @placeholder = "主机IP", @required = "required", @class = "form-control" })
        </div>
    </div>

    <div class="form-group">
        <label class="col-md-2 control-label">轮询周期</label>
        <div class="col-md-4">
            @Html.TextBoxFor(m => m.Interval, new { @placeholder = "轮询周期", @required = "required", @number = "true", @class = "form-control" })
        </div>
    </div>
</div>